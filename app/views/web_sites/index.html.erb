<h1>Listing web_sites</h1>

<table>
  <tr>
    <th>Set year</th>
    <th>Project num</th>
    <th>Year rate</th>
    <th>Service phone</th>
    <th>Name</th>
    <th>Lv</th>
    <th>City</th>
    <th>Desc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @web_sites.each do |web_site| %>
  <tr>
    <td><%= web_site.set_year %></td>
    <td><%= web_site.project_num %></td>
    <td><%= web_site.year_rate %></td>
    <td><%= web_site.service_phone %></td>
    <td><%= web_site.name %></td>
    <td><%= web_site.lv %></td>
    <td><%= web_site.city %></td>
    <td><%#= web_site.desc %></td>
    <td><%= link_to 'Show', web_site %></td>
    <td><%= link_to 'Edit', edit_web_site_path(web_site) %></td>
    <td><%= link_to 'Destroy', web_site, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Web site', new_web_site_path %>
