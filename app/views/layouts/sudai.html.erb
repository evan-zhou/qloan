		   alert("登录密码与确认密码不一致,请重新输入!");
		   return false;
	   }  
		return true;
	}
	
	

	function validateWebsite(){
		if(jQuery("#name").val() == ""){
			alert("请填写网站名称");
			return false;
		}

		if(jQuery("#telephone").val() == ""){
			alert("请填写客服电话");
			return false;
		}

		if(jQuery("#url").val() == ""){
			alert("请填写网站地址");
			return false;
		}

		if(!isurl(jQuery("#url").val())){
			alert("请填写正确的网站地址");
			return false;
		}

		return true;
	}
	
	//检查用户Email
	function checkEmail(email){
		var pattern = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(\.[a-zA-Z0-9_-])+/;   
	　　return pattern.test(email);   
	}

	
	
	function isurl(str){
		var RegUrl = new RegExp();
	    RegUrl.compile("^[A-Za-z]+://[A-Za-z0-9-_]+\\.[A-Za-z0-9-_%&\?\/.=]+$");
	    if (!RegUrl.test(str)) {
	        return false;
	    }
	    return true;
	} 
