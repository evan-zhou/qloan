<script type="text/javascript">
	(function($) { 
	$(function(){
		$(".hot-url-index .item-rank-rst").hover(
			function(){
				var websiteId=$(this).attr("attrValue");//获取当前对象的值
				var _this=$(this);//获取当前对象
				 $.ajax({
					   type: "POST",
					   url: "rateLevel.jhtml",
					   async: false,//设置ajax同步
					   data: {websiteId: websiteId	   
					   },
					   dataType: "json",
					   success: function(json){
						  if(null!=json){
						   $.each(json,function(i){	  
							  _this.parent().css("z-index","99").find(".tips-star").show();
							 	//_this.parent().find(".tips-star>ul>li").eq(0).find("span").eq(1).removeClass();
								//_this.parent().find(".tips-star>ul>li").eq(0).find("span").eq(1).addClass("item-rank-rst margin-top-2 irr-star"+json[i].popularityLevel);
							   $("#overAllLeverid").removeClass();
							   $("#overAllLeverid").addClass("item-rank-rst margin-top-2 irr-star"+json[i].overAllLever);//综合评价
							   $("#successLevelid").removeClass();
							   $("#successLevelid").addClass("item-rank-rst margin-top-2 irr-star"+json[i].successLevel);//成功等级
							   $("#popularityLevelid").removeClass();
							   $("#popularityLevelid").addClass("item-rank-rst margin-top-2 irr-star"+json[i].popularityLevel);//人气等级
							   $("#interestLevelid").removeClass();
							   $("#interestLevelid").addClass("item-rank-rst margin-top-2 irr-star"+json[i].interestLevel);	//利率等级						   
						   });
						  }else{
							  _this.parent().css("z-index","99").find(".tips-star").show();  
						  }
					   }
					});	
				
			},function(){
				$(this).parent().css("z-index","0").find(".tips-star").hide();
			}
		);
	});
	})(jQuery);
	</script>

